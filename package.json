		"axios": "^0.27.2",
		"axios-logger": "^2.6.1",
		"formik": "^2.2.9",
		"jwt-decode": "^3.1.2",
		"react": "18.0.0",
		"react-native": "0.69.4",
		"react-native-awesome-alerts": "^2.0.0",
