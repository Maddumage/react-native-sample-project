		"jest": "^26.6.3",
		"metro-react-native-babel-preset": "^0.70.3",
		"react-test-renderer": "18.0.0",
		"typescript": "^4.4.4"
	},
	"resolutions": {
		"@types/react": "^18"
	},
	"jest": {
		"preset": "react-native",
